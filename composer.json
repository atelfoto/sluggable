{
    "name": "atelfoto/sluggable",
    "description": "Sluggable plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "keywords": [
        "cakephp",
        "plugin",
        "Behavior"
    ],
    "require": {
        "cakephp/cakephp": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Sluggable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sluggable\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
